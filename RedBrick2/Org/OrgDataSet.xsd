                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        GEN_JOBS.JobNumber, GEN_JOBS.OpNumber, GEN_JOBS.WorkCenter, GEN_JOBS.JobDue, GEN_JOBS.JobStatus, GEN_JOBS.OpDue, GEN_JOBS.JobQty, 
                         GEN_JOBS.QtyComp, GEN_JOBS.PartNumber, GEN_JOBS.PartRev, GEN_JOBS.PartDescription, GEN_JOBS.RelDate, GEN_JOBS.CustomerCode, 
                         GEN_CUSTOMERS.CUSTOMER, CUT_JOBS_IGNORE.IGNID, CUT_JOBS_IGNORE.IGNDATE, CUT_JOBS_IGNORE.IGNUSR
FROM            GEN_JOBS LEFT OUTER JOIN
                         CUT_JOBS_IGNORE ON GEN_JOBS.OpNumber = CUT_JOBS_IGNORE.IGNOP AND GEN_JOBS.JobNumber = CUT_JOBS_IGNORE.IGNJOB LEFT OUTER JOIN
                         GEN_CUSTOMERS ON GEN_JOBS.CustomerCode = GEN_CUSTOMERS.CUSTNUM
WHERE        (GEN_JOBS.WorkCenter = @workc) AND (GEN_JOBS.JobStatus NOT LIKE '%COMPLETED%') AND (GEN_JOBS.PartNumber NOT IN
                             (SELECT        PARTNUM
                               FROM            CUT_CUTLISTS))
ORDER BY GEN_JOBS.OpDue, GEN_JOBS.PartNumber, GEN_JOBS.JobNumber</CommandText>
